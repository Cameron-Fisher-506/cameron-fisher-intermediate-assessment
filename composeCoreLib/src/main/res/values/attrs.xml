<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InformationView">
        <attr name="text" format="string" />
    </declare-styleable>

    <declare-styleable name="InformationLineItemView">
        <attr name="informationLineItemViewLabel" format="string" />
        <attr name="informationLineItemViewValue" format="string" />
    </declare-styleable>

    <declare-styleable name="OptionActionView">
        <attr name="optionActionViewIcon" format="reference" />
        <attr name="optionActionViewText" format="string" />
    </declare-styleable>

    <declare-styleable name="OptionActionMultiLineView">
        <attr name="optionActionMultiLineViewIcon" format="reference" />
        <attr name="optionActionMultiLineViewInformationOneText" format="string" />
        <attr name="optionActionMultilineViewInformationTwoText" format="string" />
        <attr name="optionActionMultilineViewDescriptionOneText" format="string" />
        <attr name="optionActionMultilineViewDescriptionTwoText" format="string" />
        <attr name="optionActionMultilineViewInformationViewText" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomInputView">
        <attr name="title" format="string" />
        <attr name="description" format="string" />
    </declare-styleable>

    <declare-styleable name="CollapsingToolbarView">
        <attr name="collapsingToolbarViewTitle" format="string" />
        <attr name="collapsingToolbarViewSubTitle" format="string" />
        <attr name="collapsingToolbarViewIcon" format="reference" />
    </declare-styleable>
</resources>